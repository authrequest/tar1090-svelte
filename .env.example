PUBLIC_SENTRY_DSN=
PUBLIC_SENTRY_ENABLED=true
PUBLIC_SENTRY_ENVIRONMENT=development
PUBLIC_SENTRY_RELEASE=
PUBLIC_SENTRY_TRACES_SAMPLE_RATE=1.0
PUBLIC_SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0
PUBLIC_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0
PUBLIC_SENTRY_ENABLE_LOGS=false
PUBLIC_SENTRY_SEND_DEFAULT_PII=false

# Optional server-side source for aircraft.binCraft.zst proxy endpoint
AIRCRAFT_ZST_REMOTE_URL=

# Optional base URL for upstream tar1090 db2 metadata (e.g. http://192.168.1.56/tar1090)
AIRCRAFT_DB_REMOTE_BASE_URL=http://192.168.1.56/tar1090

# Build-time only (do not expose publicly)
SENTRY_AUTH_TOKEN=
